<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hgsoft.entity.CarSpaceShowMessage" table="tb_CarSpaceShowMessage">
        <composite-id>
            <key-property name="parkNo" type="java.lang.Short">
                <column name="ParkNo" />
            </key-property>
            <key-property name="gateNo" type="java.lang.Short">
                <column name="GateNo" />
            </key-property>
            <key-property name="laneNo" type="java.lang.Short">
                <column name="LaneNo" />
            </key-property>
        </composite-id>
        <property name="initShow" type="java.lang.String">
            <column name="InitShow" length="150" />
        </property>
        <property name="isShowPosition" type="java.lang.String">
            <column name="IsShowPosition" length="2" />
        </property>
       
    </class>
</hibernate-mapping>
