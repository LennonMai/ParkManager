<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hgsoft.entity.LaneType" table="tb_LaneType" >
        <id name="laneTypeNo" type="java.lang.Short">
            <column name="LaneTypeNo" />
            <generator class="assigned"></generator>
        </id>
        <property name="laneTypeName" type="java.lang.String">
            <column name="LaneTypeName" length="16" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
