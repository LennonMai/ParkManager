<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hgsoft.entity.FreeDays" table="tb_FreeDays" >
         <composite-id>
            <key-property name="feeRuleType" type="java.lang.Integer">
                <column name="FeeRuleType" />
            </key-property>
            <key-property name="type" type="java.lang.Integer">
                <column name="Type" />
            </key-property>
            <key-property name="value" type="java.lang.String">
                <column name="Value" length="10" />
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>
