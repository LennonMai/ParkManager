<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hgsoft.entity.LaneOperatorRole" table="tb_OperatorRole" >
        <composite-id>
            <key-property name="operatorNo" type="java.lang.Integer">
                <column name="OperatorNo" />
            </key-property>
            <key-property name="roleNo" type="java.lang.Short">
                <column name="RoleNo" />
            </key-property>
        </composite-id>
    </class>
</hibernate-mapping>
