<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hgsoft.entity.CarSmallType" table="tb_CarSmallType"  lazy="false">
        <composite-id>
            <key-property name="carType" type="java.lang.Integer">
                <column name="CarType" />
            </key-property>
            <key-property name="carSmallType" type="java.lang.Integer">
                <column name="CarSmallType" />
            </key-property>
        </composite-id>
        <property name="carSmallName" type="java.lang.String">
            <column name="CarSmallName" length="20" />
        </property>
       </class>
</hibernate-mapping>
