<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hgsoft.entity.PriceRule_zd" table="tb_PriceRule_zd"  >
        <composite-id>
            <key-property name="feeRuleType" type="java.lang.Integer">
                <column name="FeeRuleType" />
            </key-property>
            <key-property name="vehTypeNo" type="java.lang.Integer">
                <column name="VehTypeNo" />
            </key-property>
        </composite-id>
        <property name="feeOfOneCycle" type="java.lang.Integer">
            <column name="FeeOfOneCycle" />
        </property>
        <property name="freeTimes" type="java.lang.Integer">
            <column name="FreeTimes" />
        </property>
        <property name="validateTrim" type="java.lang.Integer">
            <column name="ValidateTrim" />
        </property>
        <property name="invalidateTrim" type="java.lang.Integer">
            <column name="InvalidateTrim" />
        </property>
         <property name="maxFreeTimesUsedEveryTimes" type="java.lang.Integer">
            <column name="MaxFreeTimesUsedEveryTimes" />
        </property>
         <property name="maxTotalFee" type="java.lang.Integer">
            <column name="MaxTotalFee" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="remark" length="50" />
        </property>
    </class>
</hibernate-mapping>
