<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hgsoft.entity.VehClassCode" table="tb_VehClassCode" >
        <id name="vehClassNo" type="java.lang.Short">
            <column name="VehClassNo" />
            <generator class="assigned"></generator>
        </id>
        <property name="vehClassName" type="java.lang.String">
            <column name="VehClassName" length="10" not-null="true" />
        </property>
        <property name="backUp1" type="java.lang.Integer">
            <column name="BackUp1" />
        </property>
        <property name="keyCode" type="java.lang.Short">
            <column name="KeyCode" />
        </property>
    </class>
</hibernate-mapping>
