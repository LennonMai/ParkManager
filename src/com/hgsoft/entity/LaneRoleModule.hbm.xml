<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hgsoft.entity.LaneRoleModule" table="tb_RoleModule" >
        <composite-id>
            <key-property name="roleNo" type="java.lang.Short">
                <column name="RoleNo" />
            </key-property>
            <key-property name="moduleNo" type="java.lang.String">
                <column name="ModuleNo" length="10" />
            </key-property>
        </composite-id>
        <property name="privilege" type="java.lang.Short">
            <column name="Privilege" />
        </property>
    </class>
</hibernate-mapping>
