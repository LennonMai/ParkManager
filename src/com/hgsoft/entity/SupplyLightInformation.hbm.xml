<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.hgsoft.entity.SupplyLightInformation" table="tb_Supplylight" lazy="false" >
        <id name="startTime" type="java.lang.String">
            <column name="StartTime" length="8" />
            <generator class="assigned" />
        </id>
        <property name="endTime" type="java.lang.String">
            <column name="EndTime" length="8" />
        </property>
    </class>
</hibernate-mapping>
