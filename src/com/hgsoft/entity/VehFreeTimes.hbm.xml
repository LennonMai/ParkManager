<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hgsoft.entity.VehFreeTimes" table="tb_VehFreeTimes" >
        <id name="vehPlate" type="java.lang.String">
            <column name="VehPlate" length="10" />
            <generator class="assigned" />
        </id>
        <property name="year" type="java.lang.Integer">
            <column name="Year" />
        </property>
        <property name="leftFreeTimes" type="java.lang.Integer">
            <column name="LeftFreeTimes" />
        </property>
    </class>
</hibernate-mapping>
