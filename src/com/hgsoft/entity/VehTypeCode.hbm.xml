<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hgsoft.entity.VehTypeCode" table="tb_VehTypeCode">
        <id name="vehTypeNo" type="java.lang.Short">
            <column name="VehTypeNo" />
            <generator class="assigned"></generator>
        </id>
        <property name="vehTypeName" type="java.lang.String">
            <column name="VehTypeName" length="10" not-null="true" />
        </property>
         <property name="keyCode" type="java.lang.Short">
            <column name="KeyCode" />
        </property>
        <property name="backUp1" type="java.lang.Integer">
            <column name="BackUp1" />
        </property> 
    </class>
</hibernate-mapping>
