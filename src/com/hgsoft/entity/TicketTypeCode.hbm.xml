<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hgsoft.entity.TicketTypeCode" table="tb_TicketTypeCode">
        <id name="ticketTypeNo" type="java.lang.Short">
            <column name="ticketTypeNo" />
            <generator class="assigned" />
        </id>
        <property name="ticketTypeName" type="java.lang.String">
            <column name="ticketTypeName" length="30" />
        </property>
    </class>
</hibernate-mapping>
