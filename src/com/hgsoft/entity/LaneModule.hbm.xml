<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hgsoft.entity.LaneModule" table="tb_Module" >
        <id name="moduleNo" type="java.lang.String">
            <column name="ModuleNo" length="10" />
            <generator class="assigned" />
        </id>
        <property name="moduleName" type="java.lang.String">
            <column name="ModuleName" length="24" />
        </property>
        <property name="remark" type="java.lang.String">
            <column name="Remark" length="100" />
        </property>
    </class>
</hibernate-mapping>
