<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.hgsoft.entity.FunctionKey" table="tb_FunctionKey">
        <id name="functionNo" type="java.lang.Short">
            <column name="FunctionNo" />
            <generator class="assigned"></generator>
        </id>
        <property name="functionName" type="java.lang.String">
            <column name="FunctionName" length="6" not-null="true" />
        </property>
        <property name="backUp1" type="java.lang.Integer">
            <column name="BackUp1" />
        </property>
        <property name="keyCode" type="java.lang.Short">
            <column name="KeyCode" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
